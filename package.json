{
  "name": "node-amp",
  "version": "0.1.0",
  "description": "Recreating Winamp in ElectronJS",
  "main": "index.js",
  "author": "Harris Miller <harrismillerconsulting@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "start": "electron ./src/main.js",
    "start:debug": "electron ./src/main.js --debug",
    "start:watch": "electromon ./src/main.js --debug",
    "prepack": "check-for-leaks",
    "prepush": "check-for-leaks",
    "webpack": "node scripts/start.js"
  },
  "dependencies": {
    "@material-ui/core": "^3.4.0",
